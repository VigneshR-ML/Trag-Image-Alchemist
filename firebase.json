{
  "hosting": {
    "public": "static",
    "ignore": ["**/.*"],
    "rewrites": [
      {
        "source": "/static/**",
        "destination": "/static/$1"
      },
      {
        "source": "**",
        "run": {
          "serviceId": "flask-app",
          "region": "us-central1"
        }
      }
    ]
  }
}
